train_net: "models/coco/ResNet-101/faster_rcnn/train.prototxt"
base_lr: 0.02
lr_policy: "step"
gamma: 0.1
stepsize: 90000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0001
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "resnet101_faster_rcnn_coco_train"
iter_size: 2
